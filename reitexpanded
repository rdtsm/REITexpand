        th {
            background-color: #f2f2f2;
            font-weight: bold;
        }
        tr:nth-child(even) {
            background-color: #f9f9f9;
        }
        tr:hover {
            background-color: #f5f5f5;
        }
        .etf-row {
            cursor: pointer;
            font-weight: bold;
        }
        .holding-row {
            display: none; /* Hidden by default */
            background-color: #f0f8ff;
        }
        .holding-row.visible {
            display: table-row; /* Show when toggled */
        }
    </style>
    <script>
        document.addEventListener("DOMContentLoaded", function() {
            const etfRows = document.querySelectorAll(".etf-row");
            etfRows.forEach(row => {
                row.addEventListener("click", function() {
                    const etfId = this.dataset.etf;
                    const holdings = document.querySelectorAll(`.holding-row[data-etf="${etfId}"]`);
                    holdings.forEach(holding => {
                        holding.classList.toggle("visible");
                    });
                });
            });
        });
    </script>
